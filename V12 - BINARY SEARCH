#include<iostream>
using namespace std;

int binarysearch(int arr[], int n,int key){
    int start = 0, end = n-1;
    int mid = start+((end-start)/2);

    while(start<=end){
        if(arr[mid]==key){
            return mid;
        }
        if(key>arr[mid]){
            start = mid+1;
        }
        else{ 
           end = mid-1;
        }
        mid = start+((end-start)/2);
    }
    return -1;    

}

int main(){
     int arr[6]={1,2,3,4,5,9};

     int ans = binarysearch(arr,6,3);
     cout<<"the index of key is "<< ans<<endl;
}
